<div class="pt-8 pb-5">
    <header style="background-color: white;" class="transition-all ease-linear js-header">
        <div class="w-[850px] flex items-center justify-between pl-8">
            <a href="/">
                <img src="/images/logo.png" alt="Google" class="w-[92px] logo" />
            </a>

            <div
                class="flex-1 max-w-2xl bg-white border border-solid h-11 rounded-3xl border-gray-light shadow-normal-search hover:shadow-search hover:border-[#dfe1e500] focus-within:border-[#dfe1e500] focus-within:shadow-search"
            >
                <form
                    method="GET"
                    action="/search"
                    class="w-full h-full flex pr-2 pl-3.5 items-center"
                >
                    <div class="flex-1 h-[34px] m-0 pl-1 js-search-form">
                        <input
                            name="q"
                            type="text"
                            value="{{query}}"
                            class="w-full h-full text-base bg-transparent border-none outline-none js-search-input"
                        />
                        <input name="type" value="news" class="hidden" hidden />
                    </div>
                    <button type="button" class="h-full pl-2 pr-3 cursor-pointer">
                        <svg
                            class="w-6 h-6"
                            focusable="false"
                            viewBox="0 0 24 24"
                            xmlns="http://www.w3.org/2000/svg"
                        >
                            <path
                                fill="#4285f4"
                                d="m12 15c1.66 0 3-1.31 3-2.97v-7.02c0-1.66-1.34-3.01-3-3.01s-3 1.34-3 3.01v7.02c0 1.66 1.34 2.97 3 2.97z"
                            ></path>
                            <path fill="#34a853" d="m11 18.08h2v3.92h-2z"></path>
                            <path
                                fill="#fbbc05"
                                d="m7.05 16.87c-1.27-1.33-2.05-2.83-2.05-4.87h2c0 1.45 0.56 2.42 1.47 3.38v0.32l-1.15 1.18z"
                            ></path>
                            <path
                                fill="#ea4335"
                                d="m12 16.93a4.97 5.25 0 0 1 -3.54 -1.55l-1.41 1.49c1.26 1.34 3.02 2.13 4.95 2.13 3.87 0 6.99-2.92 6.99-7h-1.99c0 2.92-2.24 4.93-5 4.93z"
                            ></path>
                        </svg>
                    </button>
                    <button type="submit" class="pr-3 cursor-pointer">
                        <svg
                            class="w-6 h-6 fill-[#4285f4]"
                            focusable="false"
                            xmlns="http://www.w3.org/2000/svg"
                            viewBox="0 0 24 24"
                        >
                            <path
                                d="M15.5 14h-.79l-.28-.27A6.471 6.471 0 0 0 16 9.5 6.5 6.5 0 1 0 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z"
                            ></path>
                        </svg>
                    </button>
                </form>
            </div>
        </div>
    </header>

    <div class="text-[#70757a] mt-4 border-b border-solid border-[#ebebeb] text-[14px]">
        <div class="flex ml-44">
            <a
                href="/search?q={{query}}&type=all"
                class="text-[#5f6368] hover:text-[#1a73e8] pt-4 pr-3 pl-[10px]"
            >
                <div class="flex items-center pb-2">
                    <span class="mr-2">
                        <svg
                            class="w-4 h-4 DCxYpf"
                            focusable="false"
                            style="fill: currentColor;"
                            viewBox="0 0 24 24"
                            xmlns="http://www.w3.org/2000/svg"
                        ><path d="M0 0h24v24H0z" fill="none"></path><path
                                d="M16.32 14.88a8.04 8.04 0 1 0-1.44 1.44l5.76 5.76 1.44-1.44-5.76-5.76zm-6.36 1.08c-3.36 0-6-2.64-6-6s2.64-6 6-6 6 2.64 6 6-2.64 6-6 6z"
                            ></path></svg>
                    </span>
                    <span>All</span>
                </div>
            </a>
            <a
                href="/search?q={{query}}&type=images"
                class="text-[#5f6368] hover:text-[#1a73e8] pt-4 pr-3 pl-[10px]"
            >
                <div class="flex items-center pb-2">
                    <span class="mr-2">
                        <svg
                            class="w-4 h-4 DCxYpf"
                            style="fill: currentColor;"
                            focusable="false"
                            viewBox="0 0 24 24"
                        ><path
                                d="M14 13l4 5H6l4-4 1.79 1.78L14 13zm-6.01-2.99A2 2 0 0 0 8 6a2 2 0 0 0-.01 4.01zM22 5v14a3 3 0 0 1-3 2.99H5c-1.64 0-3-1.36-3-3V5c0-1.64 1.36-3 3-3h14c1.65 0 3 1.36 3 3zm-2.01 0a1 1 0 0 0-1-1H5a1 1 0 0 0-1 1v14a1 1 0 0 0 1 1h7v-.01h7a1 1 0 0 0 1-1V5"
                            ></path></svg>
                    </span>
                    <span>Images</span>
                </div>
            </a>
            <a
                href="/search?q={{query}}&type=videos"
                class="text-[#5f6368] hover:text-[#1a73e8] pt-4 pr-3 pl-[10px]"
            >
                <div class="flex items-center pb-2">
                    <span class="mr-2">
                        <svg
                            class="w-4 h-4 DCxYpf"
                            style="fill: currentColor;"
                            focusable="false"
                            viewBox="0 0 24 24"
                            xmlns="http://www.w3.org/2000/svg"
                        ><path clip-rule="evenodd" d="M0 0h24v24H0z" fill="none"></path><path
                                clip-rule="evenodd"
                                d="M10 16.5l6-4.5-6-4.5v9zM5 20h14a1 1 0 0 0 1-1V5a1 1 0 0 0-1-1H5a1 1 0 0 0-1 1v14a1 1 0 0 0 1 1zm14.5 2H5a3 3 0 0 1-3-3V4.4A2.4 2.4 0 0 1 4.4 2h15.2A2.4 2.4 0 0 1 22 4.4v15.1a2.5 2.5 0 0 1-2.5 2.5z"
                                fill-rule="evenodd"
                            ></path></svg>
                    </span>
                    <span>Videos</span>
                </div>
            </a>
            <a href="#" class="text-[#1a73e8] pt-4 pr-3 pl-[10px]">
                <div class="flex items-center border-b-[3px] border-solid border-[#1a73e8] pb-2">
                    <span class="mr-2">
                        <svg class="w-4 h-4 DCxYpf" focusable="false" viewBox="0 0 24 24"><path
                                fill="#34a853"
                                d="M22 12h-2v6.22c-.07.48-.51.82-1 .78h-7v2h7c1.59.05 2.92-1.17 3-2.76V12"
                            ></path><path
                                fill="#4285f4"
                                d="M19 3h-7v2h7c.49-.04.92.3 1 .78V12h2V5.76A2.93 2.93 0 0 0 19 3"
                            ></path><path
                                fill="#ea4335"
                                d="M12 3H5a2.93 2.93 0 0 0-3 2.76V12h2V5.78A.94.94 0 0 1 5 5h7V3"
                            ></path><path
                                fill="#fbbc04"
                                d="M4 12H2v6.24A2.916 2.916 0 0 0 5 21h7v-2H5c-.49.04-.92-.3-1-.78L4.01 12"
                            ></path><path
                                fill="#4285f4"
                                d="M10 7H6v6h4V7m8 4h-6v2h6v-2m0 4H6v2h12v-2m0-8h-6v2h6V7"
                            ></path></svg>
                    </span>
                    <span>News</span>
                </div>
            </a>
        </div>
    </div>

    <div class="ml-44 max-w-[652px]">
        <p class="text-[#70757a] h-[43px] text-sm leading-[43px] mb-3">About
            {{totalResults}}
            results
        </p>

        <div>
            {{#each results}}
                <div class="mb-[30px]">
                    <a href="{{this.link}}" class="link-item">
                        <span
                            class="text-[#202124] text-[12px] font-normal mb-2"
                        >{{this.source.title}}</span>
                        <h3
                            class="pt-1 mb-1 text-[20px] font-normal text-[#1a0dab]"
                        >{{this.title}}</h3>
                        <p class="text-[#4d5156] in-2-line text-[14px] mb-2">{{{this.summary}}}</p>
                        <span class="text-[#70757a] text-[14px]">1 hour ago</span>
                    </a>
                </div>
            {{/each}}
        </div>
    </div>
</div>

<script src="js/header.js" type="text/javascript"></script>